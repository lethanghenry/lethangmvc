@model IEnumerable<Basic_level2_agan.Models.Blog>

@{
    ViewBag.Title = "Index";
 
}

<!DOCTYPE html>
<html>
<head>
    <title></title>
   <style>
      body{
          padding:0px;
          margin:0px;
      }
   </style>
</head>
<body>
    <div>
        <div class="header row col-md-12" style="padding: 0px; margin: 0px; width: 100%;background:black;height:50px;">
         @Html.Partial("Header")
        </div>
        <div class="main row col-md-12"style="padding:0px;margin-right:10px;">
            <div class="menu col-md-2">
                @Html.Partial("Menu")
            </div>
            <div class="content col-md-10" style="padding:0px;margin:0px;">
                @Html.Partial("ListBlogs")
            </div>

        </div>
        <div class="col-md-12" style="padding:0px;">
            @Html.Partial("Footer")
        </div>
    </div>

</body>
</html>
